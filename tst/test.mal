fn start()
{
    clear()
    cursoff(true)
    var hits = 0
    var key = 0
    var i = 0
    while key != "#" do
    {
        key = getch()
        locate(1,1)
        if key != "" then
            write("Key Pressed %{key}.  Hits %{hits}")
        locate(10,10)
        write("Counter: %{i++}")
    }
}
sleep(1000)
